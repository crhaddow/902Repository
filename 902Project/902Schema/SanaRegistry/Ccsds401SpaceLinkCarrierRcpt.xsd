				<xsd:attribute name="value" type="Ccsds401CarrierRcptPredictedSignalLevel" use="required"/>
				<xsd:attribute name="classifier" type="xsd:string" fixed="Ccsds401CarrierRcptPredictedSignalLevel"/>
				<xsd:attribute name="oid" type="ObjectIdentifier" fixed="1.3.112.4.4.2.1.20300.1.6.1.1"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:simpleType name="Ccsds401CarrierRcptPredictedSignalLevel">
		<xsd:restriction base="xsd:integer">
			<xsd:minInclusive value="-250"/>
			<xsd:maxInclusive value="-30"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--
This parameter reports for the nominal polarization (see parameter ccsds401CarrierRcptPolarization)
 the difference between the actual and the predicted signal level of the received
 signal as observed at the LNA input in 1/100 dB (actual value minus predicted value).
 
-->
	<!--
 The engineering unit of this parameter is 1/100 dB.
 -->
	<xsd:simpleType name="Ccsds401CarrierRcptSignalLevelResidual">
		<xsd:restriction base="xsd:integer">
			<xsd:minInclusive value="-5000"/>
			<xsd:maxInclusive value="5000"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--
This parameter reports the system noise temperature in K derived from the noise density
 observed at the receiver input. As such, it takes into account all contributions
 to the noise temperature such as antenna microwave components, atmospheric noise
 and cosmic microwave background noise. The noise temperature varies with weather
 conditions and antenna elevation due to variation of the path length through the
 atmosphere and ground noise received by the antenna side lobes.
-->
	<!--
 The engineering unit of this parameter is Kelvin.
 -->
	<xsd:simpleType name="Ccsds401CarrierRcptSystemNoiseTemperature">
		<xsd:restriction base="xsd:integer">
			<xsd:minInclusive value="1"/>
			<xsd:maxInclusive value="1000"/>
		</xsd:restriction>
	</xsd:simpleType>
