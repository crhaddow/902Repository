															<!--
              The denominator required for calculating the coherent receive frequency from the transmit frequency. The value is assigned to the TURNAROUND_DENOMINATOR keyword.
              -->
															<xsd:simpleType>
																<xsd:restriction base="xsd:integer">
																	<xsd:minInclusive value="1"/>
																	<xsd:maxInclusive value="32786"/>
																</xsd:restriction>
															</xsd:simpleType>
